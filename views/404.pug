doctype html
html(lang='en')
  head
    // SITE TITTLE
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    title WellnessRoom
    // PLUGINS CSS STYLE
    link(href='/plugins/jquery-ui/jquery-ui.min.css', rel='stylesheet')
    link(href='/node_modules/bootstrap/dist/css/bootstrap.min.css', rel='stylesheet')
    link(href='/node_modules/font-awesome/css/font-awesome.min.css', rel='stylesheet')
    link(href='/plugins/selectbox/select_option1.css', rel='stylesheet')
    link(href='/plugins/slick/slick.css', rel='stylesheet', media='screen')
    link(href='/plugins/slick/slick-theme.css', rel='stylesheet', media='screen')
    link(href='/node_modules/prismjs/themes/prism-okaidia.css', rel='stylesheet')
    link(href='/node_modules/@fancyapps/fancybox/dist/jquery.fancybox.min.css', rel='stylesheet')
    link(href='/node_modules/font-awesome-animation/dist/font-awesome-animation.min.css', rel='stylesheet')
    link(href='/plugins/selectbox/select_option1.css', rel='stylesheet')
    link(href='/plugins/animate.css', rel='stylesheet')
    // GOOGLE FONT
    // CUSTOM CSS
    link(href='/css/style.css', rel='stylesheet')
    link#option_color(rel='stylesheet', href='/css/default.css')
    // FAVICON
    link(href='/img/favicon.png', rel='shortcut icon')
    // HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries
    // WARNING: Respond.js doesn't work if you view the page via file://
    //if lt IE 9
      script(src='https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js')
      script(src='https://oss.maxcdn.com/respond/1.4.2/respond.min.js')
  body#body.static
    .main-wrapper.pages.error
        section.error-page
            .container
                .row
                    .col-md-8.col-sm-10.offset-md-2.offset-sm-1
                        .error-content.text-center
                            img(src='/img/logo.png', alt='')
                            span
                                | 4
                                i.fa.fa-frown-o(aria-hidden='true')
                                | 4
                            p
                                | Oops!  Page not found
                            a.btn.btn-primary.btn-default(href='/') Come back
    // FOOTER
    footer.footer
      .container
        .row
          // BACK TO TOP BUTTON
          // <a href="#pageTop" class="backToTop"><i class="fa fa-angle-up" aria-hidden="true"></i></a>
          // NEWSLETTER
          .col-md-6.offset-md-3
            form.newsletter.text-center
              h3.col-8.offset-2.text-center Subscribe to our Newsletter
              input.clearfix(type='email', name='email', placeholder='Enter your email')
              button.btn.btn-primary.btn-default(type='submit', value='subscribe') subscribe
          // COPY RIGHT
          .col-md-6.offset-md-3
            ul.list-inline.socialLink.text-center
              li.list-inline-item
                a(href='#')
                  i.fa.fa-facebook(aria-hidden='true')
              li.list-inline-item
                a(href='#')
                  i.fa.fa-twitter(aria-hidden='true')
              li.list-inline-item
                a(href='#')
                  i.fa.fa-feed(aria-hidden='true')
              li.list-inline-item
                a(href='#')
                  i.fa.fa-vimeo(aria-hidden='true')
          .col-md-6.offset-md-3
            .copyRight_text.text-center
              p
                | WellnessRoom &copy; 2018. All Rights Reserved by 
                a(href='http://wellnessroom.ca/', target='_blank') WellnessRoom
                | .
    // JAVASCRIPTS
    script(src='https://maps.googleapis.com/maps/api/js?key=AIzaSyBEDfNcQRmKQEyulDN8nGWjLYPm8s4YB58', async='', defer='')
    script(src='/dist/deps.js')
    script(src='/plugins/jquery-ui/jquery-ui.min.js')
    script(src='/plugins/slick/slick.min.js')
    script(src='/plugins/syotimer/jquery.syotimer.js')
    script(src='/plugins/selectbox/jquery.selectbox-0.1.3.min.js')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/Counter-Up/1.0.0/jquery.counterup.min.js')
    script(src='/js/custom.js')
